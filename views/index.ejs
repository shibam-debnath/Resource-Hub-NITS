    <!-- Contact Form-->
    <section class="contact-section section_padding" id="contact">
        <!-- <div> -->
            <!-- <div class="col-20"> -->
              <h2 class="contact-title">Get in Touch</h2>
              <div style="margin-bottom: 8vh;
              text-align: center;
              font-size: 3vh;
              text-decoration: underline;">
                <%= message != '' ? message : '' %>
            </div>
            <!-- </div> -->
            <!-- <div class="col-lg-6"> -->
              <form class="form-contact contact_form" action="/contactus" method="post" id="contactForm" enctype="multipart/form-data">
                <div class="row">
                  <div class="col-12">
                    <div class="form-group">
    
                      <textarea class="form-control w-100" name="message" id="message" cols="30" rows="9"
                        onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter Message'"
                        placeholder='Enter Message' required></textarea>
                    </div>
                  </div>
                  <div class="col-sm-6">
                    <div class="form-group">
                    <% if(typeof user !== 'undefined') { %>
                      <input class="form-control" name="name" id="name" type="text" value="<%= typeof user !== 'undefined' ? user.name : '' %>" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter Name'"
                      placeholder='Enter Name' readonly>
                    <% } %>
                    <% if(typeof user === 'undefined') { %>
                        <input class="form-control" name="name" id="name" type="text" value="<%= typeof user !== 'undefined' ? user.name : '' %>" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter Name'"
                        placeholder='Enter Name'>
                    <% } %>
                    </div>
                  </div>
                  <div class="col-sm-6">
                    <div class="form-group">
                    <% if(typeof user !== 'undefined') { %>
                      <input class="form-control" name="email" id="email" type="email" value="<%= typeof user !== 'undefined' ? user.email : '' %>" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter Email'"
                      placeholder='Enter Email' readonly>
                    <% } %>
                    <% if(typeof user === 'undefined') { %>
                        <input class="form-control" name="email" id="email" type="email" value="<%= typeof user !== 'undefined' ? user.email : '' %>" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter Email'"
                        placeholder='Enter Email'>
                      <% } %>
                    </div>
                  </div>
                  <div class="col-12">
                    <div class="form-group">
                      <input class="form-control" name="subject" id="subject" type="text" onfocus="this.placeholder = ''"
                        onblur="this.placeholder = 'Enter Subject'" placeholder='Enter Subject' required>
                    </div>
                  </div>
                </div>
                <p>Upload screenshots of errors if any!</p><br>
                <label class="custom-file-upload" style="color:black !important; font-size: 15px; font-weight: bold;width: 45vw;">
          
                    <input name="issue" type="file" id="file-upload1" class="file-path validate" style="position: relative;left:0.2vw" multiple />
                    <span><i class="fas fa-upload"></i></span>
                    UPLOAD
                    <hr>
                    <span id="file-selected1"></span>
                </label>
                <p style="text-align: center; display: none;">
                    <input type="submit" value="Upload">
                </p><br>
                <div class="form-group mt-3">
                    <button type="submit" class="button button-contactForm btn_1">Send Message <i class="flaticon-right-arrow"></i> </button>
                </div>
                
              </form>
    </section>
    


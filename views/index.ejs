                console.log(a,filterValue)
                //if matched

                if(a.innerHTML.toUpperCase().indexOf(filterValue)>-1 && filterValue!=''){
                    li[i].style.display = 'block';
                }
                else{
                    li[i].style.display = 'none';
                }
            }
            
        }



